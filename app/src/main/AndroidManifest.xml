<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asus.proyectodeciclo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/soccer32"
        android:label="@string/app_name"
        android:roundIcon="@drawable/soccer32"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".Act1" />
        <activity android:name=".Act1_1" android:screenOrientation="portrait"/>
        <activity android:name=".Act1_2" />
        <activity android:name=".Act1_2_1" />
        <activity android:name=".Act1_1_1" android:screenOrientation="portrait"/>
        <activity android:name=".Act1_1_2" />
    </application>

</manifest>